			<div class="row">
	            <div class="col-lg-12">
	                <h1 class="page-header">Time-evolving label weights</h1>
	            </div>
	        </div>
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i>Labels <% if (objectResult.cperiod !== undefined) {%> from period <%= objectResult.cperiod %> <% } %>
                            <% if (objectResult.wordsTimeLine === undefined) {%>
                                <div class="pull-right">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">
                                            Sort
                                            <span class="caret"></span>
                                        </button>

                                        <ul class="dropdown-menu pull-right" role="menu">
                                            <li><a href="/timeline">Alphabetic</a>
                                            </li>
                                            <li><a href="/timeline?sort">By weight</a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            <% } else {%>
                                <div class="pull-right">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">
                                            Select period
                                            <span class="caret"></span>
                                        </button>
                                        <ul class="dropdown-menu pull-right" role="menu">
                                            <% for (var i=1; i <= objectResult.experiment; i++ ) {%>
                                            <li><a href="wordstime?period=<%= i%>">Period <%= i%></a></li>
                                        <% }%>
                                        </ul>
                                    </div>
                                </div>
                            <% } %>
                        </div>
                        	<center>
                        		<%- objectResult.svgsrc %>
                        	</center>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div id="morris-area-chart"></div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
         

	<script type="text/javascript">
			function mouseover(p) {
				var g = d3.select(this).node();
				d3.select(g).selectAll("circle").style("display","none");
				d3.select(g).selectAll("text.value").style("display","block");
			};

			function mouseout(p) {
				var g = d3.select(this).node();
				d3.select(g).selectAll("circle").style("display","block");
				d3.select(g).selectAll("text.value").style("display","none");
			};

            function mouseoverOpacity(p) {
                var g = d3.select(this).node();
                d3.select(g).style("fill-opacity","0.01");
            };

            function mouseoutOpacity(p) {
                var g = d3.select(this).node();
                d3.select(g).style("fill-opacity","0.1");
            };

			var g = d3.selectAll(".journal");
			g.on("mouseover", mouseover)
			.on("mouseout", mouseout);

            var g = d3.selectAll(".word");
            g.on("mouseover", mouseoverOpacity)
            .on("mouseout", mouseoutOpacity);

		</script>

	